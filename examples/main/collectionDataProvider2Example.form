    <ActionBlock>
      <Name value="InitializeGrid" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[
pagerListCtrl.setPageSize(5)

form.isCollection1Active = true
form.sourceDataCollection.fromJson(form.complex1.collection1.toJson())

if form.sourceDataCollection.count() > 0 then
	var first = form.sourceDataCollection.selectFirst()
	first.OrderDate = getDateTime()
endif

form.localizedColumn = "Muj sloupec"

if globals.collectionDataProvider2State = defaultString() then
	// set defaults
	form.apiobjects.listCtrl.setSorting("OrderDate:desc")

	var dt = toDate("1996-01-01", getdate())
	form.apiobjects.listCtrl.setDateRangeFilter("OrderDate", dt, defaultDate())

	form.apiobjects.listCtrl.setTextFilter("ShipCountry", "France,Austria")
	form.apiobjects.listCtrl.focusFirstRecord()
endif

form.apiobjects.listCtrl.setReady(true)
]]></Script>
