              </CssClass>
            </CustomHtml>
            <Panel>
              <Name value="customHtml11_parent" />
              <Orientation value="Vertical" />
              <Panel.Height value="#" />
              <Panel.Width value="50%" />
              <Children>
                <CustomHtml>
                  <Name value="customHtml11" />
                  <HtmlMarkup><![CDATA[<p>Otherwise, tooltip is displayed just above the mid of the button.</p>
]]></HtmlMarkup>
                  <Panel.Height value="#" />
                  <Panel.Width value="*" />
                  <Bindings>
                    <Binding SourceProperty="Data" />
                  </Bindings>
                  <CssClass>
                    <CssClassConditional>
                      <ClassName value="{theme}-customhtml" />
                      <IsThemedStyle value="True" />
                    </CssClassConditional>
                  </CssClass>
                </CustomHtml>
                <TooltipButton identity="TooltipButton.ddb64055385f4e46922b69d871374710">
                  <Name value="tooltipButton1" />
                  <Panel.Height value="#" />
                  <Panel.Width value="*" />
                  <Bindings>
                    <Binding SourceProperty="tooltip" Expression="localized.tooltip1" />
                  </Bindings>
                  <CssClass>
                    <CssClassConditional>
                      <ClassName value="{theme}-tooltipbutton" />
