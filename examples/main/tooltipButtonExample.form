<?xml version="1.0" encoding="utf-8"?>
<?data-format version="0.400"?>
<Form>
  <Name value="tooltipButtonExample" />
  <Caption value="Tooltip button example" />
  <Content>
    <Panel>
      <Name value="panel1" />
      <Orientation value="Vertical" />
      <Panel.Height value="#" />
      <Panel.Width value="*" />
      <Children>
        <HyperLink>
          <Name value="back" />
          <Data value="back" />
          <Panel.Height value="#" />
          <Panel.Width value="150px" />
          <OnClick>
            <ActionEvent ReferencedAction="+GlobalActions.goHome" />
          </OnClick>
          <CssClass>
            <CssClassConditional>
              <ClassName value="{theme}-hyperlink" />
              <IsThemedStyle value="True" />
            </CssClassConditional>
          </CssClass>
        </HyperLink>
        <TooltipButton identity="TooltipButton.ddb64055385f4e46922b69d871374710">
          <Name value="tooltipButton11" />
          <Panel.Height value="#" />
          <Panel.Width value="*" />
          <tooltipButtonAriaLabel value="Tooltip button with assistive text" />
          <Bindings>
            <Binding SourceProperty="tooltip" Expression="localized.tooltip1" />
          </Bindings>
          <CssClass>
            <CssClassConditional>
              <ClassName value="{theme}-tooltipbutton" />
              <IsThemedStyle value="True" />
            </CssClassConditional>
          </CssClass>
        </TooltipButton>
        <CustomHtml>
          <Name value="customHtml1" />
          <HtmlMarkup><![CDATA[<p>If button is very near to top/left, tooltip is displayed at left top corner.</p>
]]></HtmlMarkup>
          <Panel.Height value="#" />
          <Panel.Width value="50%" />
          <Bindings>
            <Binding SourceProperty="Data" />
          </Bindings>
          <CssClass>
            <CssClassConditional>
              <ClassName value="{theme}-customhtml" />
              <IsThemedStyle value="True" />
            </CssClassConditional>
          </CssClass>
        </CustomHtml>
