var codelistItem = form.apiobjects.Products.getItemByStringColumnAsync(form.searchByString.columnName
, form.searchByString.columnValue, relevantDate, false)
if codelistItem.isError() then
	form.searchByString.result.clear()
	form.searchByString.performedServerQuery.clear()
else
	form.searchByString.result = codelistItem.getPropertyValue(form.searchByString.resultColumnName)
	form.searchByString.performedServerQuery = codelistItem.performedServerQuery()
endif]]></Script>
