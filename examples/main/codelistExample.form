    <ActionBlock>
      <Name value="getValueByInteger" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var relevantDate = ToDateTime("2016-12-31", GetDateTime())
var codelistItem = form.apiobjects.ProductsLoaded.getItemByIntegerColumnAsync(form.searchByInt.columnName
, form.searchByInt.columnValue, relevantDate, form.searchByInt.requeryIfNotFound)
if codelistItem.isError() then
	form.searchByInt.isFound.clear()
	form.searchByInt.result.clear()
	form.searchByInt.performedServerQuery.clear()
	form.searchByInt.jsonString.clear()

	form.searchByInt.errorMessage = codelistItem.getErrorMessage()
else
	form.searchByInt.isFound = codelistItem.isFound()
	form.searchByInt.result = codelistItem.getPropertyValue(form.searchByInt.resultColumnName)
	form.searchByInt.performedServerQuery = codelistItem.performedServerQuery()
	form.searchByInt.jsonString = codelistItem.getJsonString()

	form.searchByInt.errorMessage.clear()
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="getValueByString" />
