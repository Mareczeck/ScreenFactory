      <ActionLangBody IsValid="True">
        <Script><![CDATA[var relevantDate = ToDateTime("2016-12-31", GetDateTime())
form.apiobjects.Categories.loadDataAsync(relevantDate)
form.apiobjects.Products.loadDataAsync(relevantDate)

form.categoriesAsJson = form.apiobjects.Categories.getDataAsJson()
var filtered = form.apiobjects.filteredProducts.resetFilter(true)
filtered.includeItemsByIntColumn("CategoryID", 2)
filtered.commit()

var doubleFiltered = form.apiobjects.doubleFilteredProducts.resetFilter(true)
doubleFiltered.includeItemsByIntColumn("ReorderLevel", 25)
doubleFiltered.commit()]]></Script>
